#include<iostream>
using namespace std;

int main() {
    // Step 1: Define variables
    int speed = 20;  // Speed in mph
    int time = 10;   // Time in hours
    int distance = speed * time;  // Calculate distance

    // Step 4: Display the result
    cout << "The distance traveled is: " << distance << " miles." << endl;

    return 0;
}
